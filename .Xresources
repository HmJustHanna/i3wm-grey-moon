! special
*.foreground:   #929292
*.background:   #000000
*.cursorColor:  #929292

! black
*.color0:       #000000
*.color8:       #808080

! red
*.color1:       #800000
*.color9:       #ff0000

! green
*.color2:       #008000
*.color10:      #00ff00

! yellow
*.color3:       #808000
*.color11:      #ffff00

! blue
*.color4:       #000080
*.color12:      #0000ff

! magenta
*.color5:       #800080
*.color13:      #ff00ff

! cyan
*.color6:       #008080
*.color14:      #00ffff

! white
*.color7:       #c0c0c0
*.color15:      #ffffff

! URXVT

URxvt.font:                 xft:DejaVu Sans Mono:size=14
URxvt.boldFont:             xft:DejaVu Sans Mono:bold:size=14

urxvt*scrollBar:            false
urxvt*cursorBlink:          false
urxvt*saveLines:            2000
urxvt*internalBorder:       5

!keybindings

urxvt.iso14755:                     false
urxvt.keysym.Shift-Control-V:       eval:paste_clipboard
urxvt.keysym.Shift-Control-C:       eval:selection_to_clipboard

